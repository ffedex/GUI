-- UI Close/Open keybind
-- NEBULUA KEY: PlmFx85Vxt

local RightAltButton = Instance.new("TextButton", ScreenGui)
RightAltButton.Size = UDim2.new(0, 50, 0, 25) -- Small square button
RightAltButton.Position = UDim2.new(0.5, -25, 0.5, -12)
RightAltButton.BackgroundColor3 = Color3.fromRGB(255, 165, 0) -- Orange color
RightAltButton.Text = "RAlt"
RightAltButton.Draggable = true

RightAltButton.MouseButton1Click:Connect(function()
    game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.RightAlt, false, game)
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/Feimgps/Nebula/refs/heads/main/Nebulua"))()